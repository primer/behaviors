{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["github"],
  "extends": ["plugin:github/recommended", "plugin:github/browser", "plugin:github/typescript"],
  "rules": {
    "import/extensions": ["error", "always"],
    "import/no-unresolved": "off",
    "import/no-named-as-default": "off"
  },
  "ignorePatterns": ["dist"]
}
